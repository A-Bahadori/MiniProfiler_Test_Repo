@model MiniProfiler_Test.Areas.Admin.Models.DashboardSidebarVM

<div class="col-md-3 col-lg-2 dashboard-sidebar">
    <div class="d-flex flex-column">
        @foreach (var item in Model.MenuItems)
        {
            <a href="@item.Url" class="nav-link @(item.IsActive ? "active" : "")">
                <i class="@item.Icon"></i>
                @item.Title
            </a>
        }
    </div>
</div>